<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxWidth="510.0" minHeight="200.0" minWidth="200.0" prefHeight="442.0"
            prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171"
            fx:controller="pl.edu.agh.plonka.bartlomiej.diseasesdiagnoser.view.controller.RuleEditDialogController">
    <children>
        <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                  AnchorPane.topAnchor="0.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="110.0" minWidth="110.0" prefWidth="110.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="20.0" minHeight="20.0" prefHeight="20.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="40.0" minHeight="40.0" prefHeight="40.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <VBox prefHeight="402.0" prefWidth="110.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
                    <children>
                        <Button mnemonicParsing="false" onAction="#handleAddAge" prefHeight="25.0" prefWidth="120.0"
                                text="Age">
                            <VBox.margin>
                                <Insets bottom="3.0" left="5.0" right="5.0" top="3.0"/>
                            </VBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#handleAddHeight" prefHeight="25.0" prefWidth="120.0"
                                text="Height">
                            <VBox.margin>
                                <Insets bottom="3.0" left="5.0" right="5.0" top="3.0"/>
                            </VBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#handleAddWeight" prefHeight="25.0" prefWidth="120.0"
                                text="Weight">
                            <VBox.margin>
                                <Insets bottom="3.0" left="5.0" right="5.0" top="3.0"/>
                            </VBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#handleAddSymptoms" prefHeight="25.0"
                                prefWidth="120.0" text="Symptom">
                            <VBox.margin>
                                <Insets bottom="3.0" left="5.0" right="5.0" top="5.0"/>
                            </VBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#handleAddNegativeTests" prefHeight="25.0"
                                prefWidth="120.0" text="Negative test">
                            <VBox.margin>
                                <Insets bottom="3.0" left="5.0" right="5.0" top="3.0"/>
                            </VBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#handleAddPreviousDiseases" prefHeight="25.0"
                                prefWidth="120.0" text="Previous disease">
                            <VBox.margin>
                                <Insets bottom="3.0" left="5.0" right="5.0" top="3.0"/>
                            </VBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#handleAddDiseases" prefHeight="25.0"
                                prefWidth="120.0" text="Disease">
                            <VBox.margin>
                                <Insets bottom="3.0" left="5.0" right="5.0" top="3.0"/>
                            </VBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#handleAddTreatments" prefHeight="25.0"
                                prefWidth="120.0" text="Treatment">
                            <VBox.margin>
                                <Insets bottom="3.0" left="5.0" right="5.0" top="3.0"/>
                            </VBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#handleAddTests" prefHeight="25.0" prefWidth="120.0"
                                text="Tests">
                            <VBox.margin>
                                <Insets bottom="3.0" left="5.0" right="5.0" top="3.0"/>
                            </VBox.margin>
                        </Button>
                    </children>
                </VBox>
                <HBox alignment="CENTER_RIGHT" prefHeight="40.0" prefWidth="429.0" GridPane.rowIndex="3">
                    <children>
                        <Button mnemonicParsing="false" onAction="#handleOK" prefHeight="25.0" prefWidth="70.0"
                                text="OK">
                            <HBox.margin>
                                <Insets bottom="5.0" left="5.0" right="3.0" top="5.0"/>
                            </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#handleCancel" prefHeight="25.0" prefWidth="70.0"
                                text="Cancel">
                            <HBox.margin>
                                <Insets bottom="5.0" left="3.0" right="5.0" top="5.0"/>
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
                <TextArea fx:id="ruleViewArea" editable="false" prefHeight="200.0" prefWidth="200.0" wrapText="true"
                          GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets bottom="2.0" left="4.0" right="5.0" top="2.0"/>
                    </GridPane.margin>
                </TextArea>
                <Label text="Rule name">
                    <GridPane.margin>
                        <Insets bottom="2.0" left="10.0" right="5.0" top="2.0"/>
                    </GridPane.margin>
                </Label>
                <TextField fx:id="ruleNameField" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets bottom="2.0" left="4.0" right="5.0" top="2.0"/>
                    </GridPane.margin>
                </TextField>
            </children>
        </GridPane>
    </children>
</AnchorPane>
